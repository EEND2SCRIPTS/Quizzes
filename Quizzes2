local CustomCategoryManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/Damian-11/quizbot/master/quizbot.luau"))()

local FootballEasy = CustomCategoryManager.New("Football/Soccer", "easy")
FootballEasy:Add("Which country won the FIFA World Cup in 2014?",{"Germany", "Brazil", "Argentina", "Spain"})
FootballEasy:Add("Who is known as 'The King of Football'?",{"Lionel Messi", "Pele", "Cristiano Ronaldo", "Diego Maradona"}, 2)
FootballEasy:Add("What is the maximum number of players allowed on the field for one team during a soccer match?",{"11", "10", "9", "12"})
FootballEasy:Add("Which position is primarily responsible for preventing the opposing team from scoring?",{"Goalkeeper", "Midfielder", "Defender", "Forward"})

local FootballHard = CustomCategoryManager.New("Football/Soccer", "hard")
FootballHard:Add("Which player holds the record for the most goals scored in a single calendar year, with 91 goals in 2012?",{"Lionel Messi", "Gerd Muller", "Cristiano Ronaldo", "Pele"})
FootballHard:Add("What is the only country to have played in every FIFA World Cup since its inception in 1930?",{"Brazil", "Germany", "Argentina", "Italy"})
    FootballHard:Add("Which club won the UEFA Champions League in the 1999 season with a dramatic late comeback in the final?",{"Manchester United", "AC Milan", "Real Madrid", "Barcelona"}, 2)
FootballHard:Add("Who is the all-time top scorer in the UEFA Champions League as of 2023?",{"Cristiano Ronaldo", "Lionel Messi", "Raul", "Robert Lewandowski" })
FootballHard:Add("Which country hosted the 2010 FIFA World Cup, becoming the first African nation to do so?",{"South Africa", "Nigeria", "Ghana", "Egypt"})

local Economics = CustomCategoryManager.New("Economics")
Economics:Add("What is GDP?",{"Gross Domestic Product", "General Development Plan", "Goverment Debt Policy", "Global Demand Process"})
Economics:Add("Which of the following is a key principle of supply and demand?",{"Prices decrease as supply increases", "Demand always exceeds supply", "Prices increase as demand decreases", "Supply always matches demand perfectly"}, 2)
Economics:Add("What does inflation refer to?",{"An increase in the general price level of goods and services.", "A decrease in the general price level of goods and services", "A stable economy with no price changes.", "A measure of economic growth"})
Economics:Add("Which of the following is considered a type of market structure?",{"Monopoly", "Democracy", "Regulation", "Inflation"})
Economics:Add("What is the purpose of a central bank?",{"To issue currency and manage monetary policy", "To regulate the stock market", "To control government spending", "To oversee international trade agreements"})

local ScreamFranchise = CustomCategoryManager.New("ScreamFranchise")

