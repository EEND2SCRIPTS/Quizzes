local MathExtremelyHard = CustomCategoryManager.New("MathExtremelyHard")

MathExtremelyHard:Add("Algebra: What are the roots of the equation x4-4x3+6x2-4x+1=0?",{"1,1,1,1", "2,2,0,0", "3,-1,1,-1", "0,0,2,2"})
MathExtremelyHard:Add("Which of the following numbers is prime?",{"97", "91", "77", "51"})
MathExtremelyHard:Add("Linear Algebra: What is the determinant of the matrix B=(1 2 3 4)?",{"-10", "10", "7", "-7"}, 2)
MathExtremelyHard:Add("Combinatorics: How many distinct permutations can be made from the letters in the word 'BANANA'?",{"30", "60", "20", "15"})
MathExtremelyHard:Add("Evaluate the following definite integral: 3 down to 1 (4xE3-2xE2+x)dx",{"22", "30", "16", "18"}, 3)

local Sayings And IdiomsHard = CustomCategoryManager.New("Sayings And Idioms", "hard")

